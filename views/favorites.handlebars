<h1 class="text-center">My Events</h1>
<div class="container">
    <div class="row mt-4">
        <div class="col-md">
            <div class="card-group">

                <!-- Card Dark -->
                <div class="card">

                    <!-- Card image -->
                    <div class="view overlay">
                        <img class="card-img-top" src={{imageUrl}} alt="Card image cap">
                        <a>
                            <div class="mask rgba-white-slight"></div>
                        </a>
                    </div>

                    <!-- Card content -->
                    <div class="card-body elegant-color white-text rounded-bottom">

                        <!-- Social shares button -->
                        <a class="activator waves-effect mr-4">
                            <i class="fa fa-share-alt white-text"></i>
                        </a>
                        <!-- Title -->
                        <h4 class="card-title">Card title</h4>
                        <hr class="hr-light">
                        <!-- Text -->
                        <p class="card-text white-text mb-4">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                        <!-- Link -->
                        <a href="#!" class="white-text d-flex justify-content-end">
                            <h5>Read more
                                <i class="fa fa-angle-double-right"></i>
                            </h5>
                        </a>

                    </div>

                </div>
                <!-- Card Dark -->

            </div>
        </div>

    </div>

</div>

<ul>
    {{#each favoritesStuff}}
    <li>{{this}}</li>
    {{/each}}
</ul>


